      "required": true
    },
    "edit": {
      "default": "",
      "maxLength": 50,
      "type": "string"
    },
    "likes": {
      "collection": "like",
      "via": "track"
    },
    "posts": {
      "collection": "post",
      "via": "track"
    },
    "label": {
      "model": "label",
