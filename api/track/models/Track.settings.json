    },
    "explicit": {
      "default": false,
      "type": "boolean",
      "required": true
    },
    "audio": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "description": {
      "default": "",
      "type": "text"
    },
    "profiles": {
      "collection": "profile",
      "dominant": true,
